@page
@model RegisterModel
@{
    ViewData["Title"] = "Register";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!-- Outer Row -->
<div class="row justify-content-center">

    <div class="col-xl-10 col-lg-12 col-md-9">
        <div class="card o-hidden border-0 shadow-lg my-5">
            <div class="card-body p-0">
                <!-- Nested Row within Card Body -->
                <div class="row">
                    <div class="row">
                        <div class="col-lg-6 d-none d-lg-block bg-login-image" style=" background: black url(https://png.pngtree.com/png-clipart/20230131/ourlarge/pngtree-cute-bear-png-image_6581260.png);
            background-position: center;
            background-size: contain;
            background-repeat: no-repeat;"></div>
                        <div class="col-md-6">

                            <form asp-route-returnUrl="@Model.ReturnUrl" method="post">
                                <h4 class="text-center">Điền thông tin để tạo tài khoản mới.</h4>
                                <hr />
                                <div asp-validation-summary="All" class="text-danger"></div>
                                <div class="form-group">
                                    <label asp-for="Input.Firstname"></label>
                                    <input asp-for="Input.Firstname" class="form-control" />
                                    <span asp-validation-for="Input.Firstname" class="text-danger"></span>
                                </div>
                                <div class="form-group">
                                    <label asp-for="Input.LastName"></label>
                                    <input asp-for="Input.LastName" class="form-control" />
                                    <span asp-validation-for="Input.LastName" class="text-danger"></span>
                                </div>
                                <div class="form-group">
                                    <label asp-for="Input.PhoneNumber"></label>
                                    <input asp-for="Input.PhoneNumber" class="form-control" />
                                    <span asp-validation-for="Input.PhoneNumber" class="text-danger"></span>
                                </div>
                                <div class="form-group">
                                    <label asp-for="Input.Email"></label>
                                    <input asp-for="Input.Email" class="form-control" />
                                    <span asp-validation-for="Input.Email" class="text-danger"></span>
                                </div>
                                <div class="form-group">
                                    <label asp-for="Input.Password"></label>
                                    <input asp-for="Input.Password" class="form-control" />
                                    <span asp-validation-for="Input.Password" class="text-danger"></span>
                                </div>
                                <div class="form-group">
                                    <label asp-for="Input.ConfirmPassword"></label>
                                    <input asp-for="Input.ConfirmPassword" class="form-control" />
                                    <span asp-validation-for="Input.ConfirmPassword" class="text-danger"></span>
                                </div>
                                <br />
                                <button type="submit" class="btn btn-primary">Đăng ký</button>
                            </form>
                        </div>
                    </div>
                    <div class="col-md-6 col-md-offset-2">
                        @if ((Model.ExternalLogins?.Count ?? 0) != 0)
                        {
                            <section>
                                <h4>Sử dụng dịch vụ</h4>
                                <hr />
                                <form id="external-account" asp-page="./ExternalLogin" asp-route-returnUrl="@Model.ReturnUrl" method="post" class="form-horizontal">
                                    <div>
                                        <p>
                                            @foreach (var provider in Model.ExternalLogins)
                                            {
                                                <button type="submit" class="btn btn-primary" name="provider" value="@provider.Name" title="Log in using your @provider.DisplayName account">@provider.DisplayName</button>
                                            }
                                        </p>
                                    </div>
                                </form>
                            </section>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>
<script>
    const imageUrls = [
        'https://pdp.edu.vn/wp-content/uploads/2021/06/hinh-anh-anime-ac-quy-kinh-di-dang-so-nhat-820x513.png',
        'https://lh6.googleusercontent.com/a4XpRBMp1z8nNBnbYdiLuFEWVs39Gq3j0KYgb6bKHJEBnS_8CgNu7tFpIIjb9KyLw9v546oLcaJUpLQqnBsRwI-MDEoF7qya8V8UTGq_gWBK_qxuYanMj7L-9cN5oAmYzaaPdzZN',
        'https://www.pngkey.com/png/full/43-432075_elizabeth-gintama-elizabeth-gintama-png.png',
        'https://anhdep123.com/wp-content/uploads/2020/09/anh-chibi-tinh-yeu.png',
        'https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTk4JI9Rmt_vlsrMuX5XsC91gOAg_LsSGdKDw&usqp=CAU',
        'data:image/jpeg;base64,',
        'https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRecoGWCfTQutm9Jh9h-E_h-U3qB4HRZIXp2w&usqp=CAU',
        'https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQU8TDDFS503rPt6U-CNt2J8eN65YJMoljLhw&usqp=CAU',
        'https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSProvni6-_X6vpmRNHypr0rnb1tp2vWCTS7g&usqp=CAU',
        'https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRCBSby-Eog7rtsX6n7KnopJpVFmkJjMcScJA&usqp=CAU',
        'https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTI3VyFSaUeXllGBRwpqpVoTtkmFcZpO2pbOQ&usqp=CAU',
        'https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRRP9vyiBm1Uuk2ktY6AYGIZadBvAtyWvN4z0wmXXayJUrcqlMrEEtTFF4h9oqMBp3qADE&usqp=CAU',
        'https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTFFE7Su01dk2wLr2GY7sVFL3yq6iQhvJSBlQ&usqp=CAU'
    ];

    let currentImageIndex = 0;
    const imageElement = document.querySelector('.bg-login-image');

    setInterval(() => {
        currentImageIndex = (currentImageIndex + 1) % imageUrls.length;
        const newImageUrl = imageUrls[currentImageIndex];
        imageElement.style.backgroundImage = `url(${newImageUrl})`;
    }, 1500);

</script>
@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}